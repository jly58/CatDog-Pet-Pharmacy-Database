  <h1>Browse Vetenerians</h1>
  {% include 'navbar.html' %}

  <table>
    <table border="1" cellpadding="5">
    <thead>
    <tr>
        <!-- Iterate through each key in the first entry to get the
        column name -->
        {% for key in Vets[0].keys() %}

        <!-- Create a <th> tag with the key inside of it, this will be
        our header row -->
        <th>{{ key }}</th>

        <!-- End of this for loop -->
        {% endfor %}
    </tr>
    <tbody>

    <!-- Now, iterate through every vetenerian in Vets -->
    {% for vetenerian in Vets %}
    <tr>
        <!-- Then iterate through every key in the current vetenerian dictionary -->
        {% for key in vetenerian.keys() %}

        <!-- Create a <td> element with the value of that key in it -->
        <td>{{vetenerian[key]}}</td>
        {% endfor %}
    </tr>
    {% endfor %}
    </table>

<a href="/add_vet">Add a vetenerian</a>
<a href="/del_vet">Delete a vetenerian</a>